{
  "name": "gatsby-markdown-material-typescript-starter",
  "description": "GatsbyJS Markdown Material TypeScript Blog Starter.",
  "version": "1.1.0",
  "author": "Stephen Harrison <stephen@stephen.cloud>",
  "dependencies": {
    "@material-ui/core": "^4.10.2",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/styles": "^4.10.0",
    "@types/node": "^14.0.14",
    "@types/react": "^16.9.41",
    "@types/react-dom": "^16.9.8",
    "@types/url-join": "^4.0.0",
    "acorn": "^7.3.1",
    "eslint-config-airbnb-typescript": "^8.0.2",
    "gatsby": "^2.23.12",
    "gatsby-cli": "^2.12.48",
    "gatsby-plugin-catch-links": "^2.3.5",
    "gatsby-plugin-feed": "^2.5.5",
    "gatsby-plugin-google-analytics": "^2.3.4",
    "gatsby-plugin-htaccess": "^1.4.0",
    "gatsby-plugin-manifest": "^2.4.12",
    "gatsby-plugin-nprogress": "^2.3.4",
    "gatsby-plugin-offline": "^3.2.11",
    "gatsby-plugin-react-helmet": "^3.3.4",
    "gatsby-plugin-sass": "^2.3.4",
    "gatsby-plugin-sharp": "^2.6.12",
    "gatsby-plugin-sitemap": "^2.4.5",
    "gatsby-plugin-twitter": "^2.3.4",
    "gatsby-plugin-typography": "^2.5.6",
    "gatsby-remark-autolink-headers": "^2.3.5",
    "gatsby-remark-copy-linked-files": "^2.3.5",
    "gatsby-remark-images": "^3.3.11",
    "gatsby-remark-prismjs": "^3.5.4",
    "gatsby-remark-responsive-iframe": "^2.4.5",
    "gatsby-source-filesystem": "^2.3.12",
    "gatsby-transformer-remark": "^5.25.1",
    "gatsby-transformer-sharp": "^2.5.5",
    "node-sass": "^4.14.1",
    "prismjs": "^1.20.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "react-refresh": "^0.8.3",
    "react-share": "^4.2.0",
    "react-twitter-widgets": "^1.9.5",
    "react-typography": "^0.16.19",
    "typography": "^0.16.19",
    "typography-theme-github": "^0.16.19",
    "typography-theme-stow-lake": "^0.16.19"
  },
  "devDependencies": {
    "@types/react-helmet": "^5.0.15",
    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "@typescript-eslint/parser": "^2.28.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "gatsby-plugin-material-ui": "^2.1.9",
    "graphql": "^14.6.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.4",
    "source-map-support": "^0.5.16",
    "ts-node": "^8.8.2",
    "typescript": "^3.8.3"
  },
  "keywords": [
    "gatsby",
    "typescript",
    "material-ui"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "develop": "gatsby develop",
    "dev": "npm run develop",
    "serve": "gatsby serve",
    "build": "gatsby build",
    "build:pp": "gatsby build --prefix-paths",
    "build:gh": "npm run clean && npm run build:pp && gh-pages -d public",
    "clean": "rm -rf public && rm -rf .cache",
    "lint:js": "eslint --ext .js",
    "lint:md": "remark content/posts/",
    "write-good": "write-good $(glob 'content/posts/**/*.md')",
    "format:js": "prettier '**/*.{js,jsx}' --write",
    "type-check": "tsc --noEmit",
    "deploy": "gatsby build && gh-pages -d public -b master"
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended"
    ]
  }
}
